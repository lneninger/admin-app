{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[Admin]Launch Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      // "sourceMapPathOverrides": {
      //   "webRoot": "${workspaceRoot}",
      //   "webpack:/*": "${workspaceRoot}/*"
      // }
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**/*.js"]
    },
    {
      "name": "[Admin]Firebase Functions Emulator",
      "port": 5001,
      "request": "attach",
      "type": "node",
      "webRoot": "${workspaceFolder}/functions",
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**/*.js"]
    },
  {
    "name": "Firestore functions",
    "port": 9229,
    "request": "attach",
    "type": "node"
  },
  {
    "type": "pwa-chrome",
    "request": "attach",
    "name": "[Benelynk]Attach to Karma",
    "address": "localhost",
    "port": 9222,
    "timeout": 600000,
    "sourceMaps": true,
    "webRoot": "${workspaceFolder}",
    "pathMapping": {
      "/_karma_webpack_": "${workspaceFolder}"
    }
  }
  ]
}
