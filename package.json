  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start-tailwind": "ng serve --open & ngtw watch",
    "start-fake": "ng serve -c serverfake --open & ngtw watch",
    "server-fake": "json-server --watch server-fake/db.json",
    "serve": "ng serve",
    "ngtw": "ngtw watch",
    "theme": "sass --watch --style=compressed --no-source-map src/app/assets/themes:src/assets/themes",
    "build": "ngtw build && ng build",
    "build-prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "global-packages": "npm list -g --depth 0",
    "publish": "npm run build-prod && firebase deploy --only hosting",
    "port-process": "netstat -a -n -o | find \"port\"",
    "kill-process": "taskkill /F /PID pid_number"
  },
  "private": true,
