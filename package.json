{
  "name": "quickapp",
  "version": "3.0.0",
  "description": "ASP.NET Core 3.0/Angular 8 startup project template with complete login, user and role management. Plus other useful services for Quick Application Development",
  "author": {
    "name": "Ebenezer Monney",
    "email": "info@ebenmonney.com",
    "url": "https://www.ebenmonney.com/about"
  },
  "license": "MIT",
  "homepage": "https://www.ebenmonney.com/quickapp",
  "repository": "https://github.com/emonney/quickapp.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open & ngtw",
    "start-fake": "ng serve -c serverfake --open & ngtw",
    "server-fake": "json-server --watch server-fake/db.json",
    "serve": "ng serve",
    "ngtw": "ngtw watch",
    "theme": "sass --watch --style=compressed --no-source-map src/app/assets/themes:src/assets/themes",
    "build": "ngtw build && ng build",
    "build-prod": "ngtw build --purge && ng build -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "global-packages": "npm list -g --depth 0",
    "publish": "npm run build-prod && firebase deploy --only hosting"
  },
  "private": true,
  "dependencies": {
